@echo off
set PATH=%SYSTEMROOT%\SYSTEM32;%SYSTEMROOT%;%SYSTEMROOT%\SYSTEM32\WBEM;

set  HELP=Cleaning hosts file
set USAGE=

call :Cleaning_Hosts
goto Finish


:Cleaning_Hosts
                                             set HOSTS_PATH=System32\drivers\etc
if exist "%SYSTEMROOT%\SysWOW64\drivers\etc" set HOSTS_PATH=SysWOW64\drivers\etc
                                             set HOSTS_FILE=%SYSTEMROOT%\%HOSTS_PATH%\hosts
call>nul 2>nul :Cleaning_Hosts_Sub || goto Cleaning_Hosts_Fail
type 2>nul "%HOSTS_FILE%"|%SystemRoot%\System32\find.exe>nul 2>nul /I "Cleaned by CleanHosts: %DATE%" && (
  call EchoSucc Cleaning hosts
  exit /B 0
)
:Cleaning_Hosts_Fail
call EchoFail Cleaning hosts
if /I "%DEBUG%"=="YES" call :Cleaning_Hosts_Sub
exit /B 1
:Cleaning_Hosts_Sub
call SetAccessMode     "%HOSTS_FILE%" full
attrib.exe +A -H -R -S "%HOSTS_FILE%"
echo> "%HOSTS_FILE%" # Cleaned by CleanHosts: %DATE% %TIME%
echo>>"%HOSTS_FILE%" 127.0.0.1  localhost
call>nul CheckDC && goto Cleaning_Hosts_Sub_Skip
